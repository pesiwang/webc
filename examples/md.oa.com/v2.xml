<webc>
	<!--{第一部分,版本定义}-->
	<version>2</version>

	<!--{第二部分,定义结构体}-->
	<objects>
		<struct name="user_login_request">
			<string name="mobile"/>
			<string name="password"/>
		</struct>
		<struct name="user_login_response">
			<string name="pauth"/>
		</struct>
		<struct name="foo_bar_request">
			<array name="something" reference="haha,user_login_request"/>
			<struct name="something1" reference="haha"/>
			<struct name="somethingelse">
				<string name="aaa"/>
				<bool name="bbb"/>
			</struct>
		</struct>
		<struct name="haha">
			<integer name="aa"/>
		</struct>
	</objects>

	<!--{第三部分,定义web接口列表}-->
	<interfaces>
		<interface name="foo" request="foo_bar_request" response=""/><!--FooBar-->
		<interface name="user.login" request="user_login_request" response="user_login_response"/><!--用户登录-->
	</interfaces>
</webc>
